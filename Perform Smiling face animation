#include<iostream.h> 
#include<conio.h> 
#include<graphics.h> 
void main() 
 { 
int gd=DETECT,gm,i; 
initgraph(&gd,&gm,"c:\\tc\\bgi"); 
//face 
circle(250,250,150); 
setfillstyle(1,5); 
floodfill(225,225,15); 
// Two Eyes 
circle(180,200,20); 
setfillstyle(1,2); 
floodfill(181,200,15); 
circle(300,200,20); 
setfillstyle(1,2); 
floodfill(300,200,15); 
//mouth 
arc(250,270,180,360,80); 
line(170,270,330,270); 
setfillstyle(1,4);
//eye brow 
arc(180,190,10,170,30); OUTPUT 
arc(300,190,10,170,30); 
//Ear draw 
arc(390,250,270,90,50); 
setfillstyle(1,6); 
floodfill(402,250,15); 
arc(110,250,90,270,50); 
setfillstyle(1,6); 
floodfill(90,250,15); 
//Hair 
line(250,100,300,40); 
line(250,100,300,60); 
getch(); 
}
